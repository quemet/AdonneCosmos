{
	"scopeName": "main.csm",
	"name": "Cosmos",
	"patterns": [
		{
			"name": "keyword.control.cosmos",
			"match": "\\b(Afficher|Effacer l'écran)\\b"
		},
		{
			"name": "comment.line.double-slash.cosmos",
			"match": "//.*"
		},
		{
			"name": "comment.block.cosmos",
			"begin": "/\\*",
			"end": "\\*/"
	  	},
		{
			"name": "variable.cosmos",
			"match": "#[a-zA-Z_][a-zA-Z0-9_]*"
		},
		{
			"name": "keyword.operator.arithmetic.cosmos",
			"match": "[+\\-*/%^]"
		},
		{
			"name": "string.cosmos",
			"begin": "[\"]",
			"end": "[\"]"
		},
		{
			"name": "keyword.operator.logical.cosmos",
			"match": " <= | < | > | >= | = | == | est égale à | n'est pas égale à | est supérieure à | est inféerieur à | est supérieure ou égale à | est plus petit ou égale à "
		}
	],
	"repository": {},
	"injections": {}
}